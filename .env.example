VITE_ANTHROPIC_API_KEY=sk-ant-your-key-here

# Dev Mode: Set to true to use mock data and bypass auth
# VITE_DEV_MODE=true

# Optional: Analytics
VITE_POSTHOG_KEY=your-posthog-key
VITE_POSTHOG_HOST=https://app.posthog.com
